

//二分查找首先是有序的

void ave_search(int nums[], int Target) { 
  int left = 0;
  int right = nums.size() - 1;  
  while (left <= right) {
    int middle = left + (right-left)>>1;
    if (nums[middle] > Target) {
      right = middle + 1;
    } else { // nums[middle] < Target
      left = left - 1;
    }
  }
  return left;
}
